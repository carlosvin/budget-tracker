{
    "name": "budget-tracker",
    "version": "3.9.5",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4",
        "@material-ui/icons": "^4",
        "@material-ui/lab": "^4.0.0-alpha.40",
        "currencies-map": "^3.2.1",
        "firebase": "^7",
        "firebaseui": "^4",
        "idb": "^5",
        "nanoid": "^3",
        "react": "^16",
        "react-dom": "^16",
        "react-frappe-charts": "^2",
        "react-router-dom": "^5"
    },
    "devDependencies": {
        "@types/jest": "^26",
        "@types/nanoid": "^2",
        "@types/node": "^14",
        "@types/react": "^16",
        "@types/react-dom": "^16",
        "@types/react-router-dom": "^5",
        "@types/react-test-renderer": "^16",
        "filesystem": "^1",
        "husky": "^4",
        "react-scripts": "^4.0.1",
        "react-test-renderer": "^16",
        "serve": "*",
        "source-map-explorer": "^2",
        "typescript": "^4"
    },
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js' --html reports/bundle-size.html",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "lint": "eslint --max-warnings 0 --ext js,ts,tsx src",
        "coverage": "yarn test --coverage --coverageDirectory=reports/coverage --watchAll=false",
        "eject": "react-scripts eject",
        "serve": "serve -s build",
        "predeploy": "yarn build",
        "deploy-redirect": "gh-pages -d .github/redirect"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn lint && yarn test --watchAll=false"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/"
        ]
    }
}
