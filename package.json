{
    "name": "budget-tracker",
    "version": "3.8.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4",
        "@material-ui/icons": "^4",
        "@material-ui/lab": "^4.0.0-alpha.32",
        "currencies-map": "^3.2.1",
        "firebase": "^7.5.0",
        "firebaseui": "^4.3.0",
        "grpc": "1.24.2",
        "idb": "^4",
        "nanoid": "^2",
        "react": "^16",
        "react-dom": "^16",
        "react-frappe-charts": "^2",
        "react-router-dom": "^5"
    },
    "devDependencies": {
        "@firebase/testing": "^0.16.0",
        "@types/jest": "^24",
        "@types/nanoid": "^2",
        "@types/node": "^12",
        "@types/react": "^16",
        "@types/react-dom": "^16",
        "@types/react-router-dom": "^4",
        "@types/react-test-renderer": "^16",
        "@types/victory": "^31",
        "filesystem": "^1",
        "gh-pages": "2.0.1",
        "husky": "^3",
        "react-scripts": "^3",
        "react-test-renderer": "^16",
        "serve": "*",
        "source-map-explorer": "^2",
        "typescript": "^3"
    },
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js' --html reports/bundle-size.html",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "lint": "eslint --max-warnings 0 --ext js,ts,tsx src",
        "coverage": "yarn test --coverage --coverageDirectory=reports/coverage --watchAll=false",
        "eject": "react-scripts eject",
        "serve": "serve -s build",
        "predeploy": "yarn build",
        "deploy-redirect": "gh-pages -d .github/redirect"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn lint && yarn test --watchAll=false"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/"
        ]
    }
}
